{
    "name": "doxygen-generator",
    "displayName": "Doxygen Generator",
    "description": "Generate doxygen for the function under the cursor",
    "version": "0.1.0",
    "publisher": "trond-snekvik",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "doxygen-generator.generate",
            "title": "Doxygen: Generate"
        }],
        "keybindings":[
            {
                "command": "doxygen-generator.generate",
                "key": "alt+Q"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "license": "MIT",
    "repository": {
        "url": "https://github.com/trond-snekvik/vscode-doxygen-generator",
        "type": "git"
    }
}